{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "luna",
      "camelcase": "Luna",
      "title": "Luna",
      "scope": "source.luna",
      "file-types": [
        "luna"
      ],
      "injection-regex": "^luna$",
      "class-name": "TreeSitterLuna",
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm",
      "folds": "queries/folds.scm",
      "injections": "queries/injections.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Luna grammar for tree-sitter",
    "authors": [
      {
        "name": "Justin Sonntag"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-luna"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
